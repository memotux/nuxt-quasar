{
  "name": "nuxt-quasar-vite",
  "version": "1.0.11",
  "description": "Quasar Framework UI for Nuxt",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:generate": "nuxi generate playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.5.2"
  },
  "devDependencies": {
    "@types/node": "^18",
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.4.0",
    "@nuxt/schema": "^3.5.2",
    "@nuxt/test-utils": "^3.5.1",
    "changelogen": "^0.5.3",
    "eslint": "^8.42.0",
    "nuxt": "^3.5.1",
    "vitest": "^0.31.4"
  },
  "author": {
    "email": "romeo@mendezfuentes.net",
    "name": "Romeo Mendez Fuentes",
    "url": "https://romeo.mendezfuentes.net"
  },
  "homepage": "https://github.com/memotux/nuxt-quasar",
  "repository": {
    "type": "git",
    "url": "https://github.com/memotux/nuxt-quasar.git"
  },
  "keywords": [
    "nuxt",
    "quasar",
    "module",
    "vite",
    "ui"
  ],
  "peerDependencies": {
    "quasar": "^2.7.1",
    "sass": "1.32.12"
  }
}