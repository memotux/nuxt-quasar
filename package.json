{
  "name": "nuxt-quasar-vite",
  "version": "1.0.14",
  "description": "Quasar Framework UI for Nuxt",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "pn prepack && pn changelogen --release && pn publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.13.2"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.6.0",
    "@nuxt/eslint-config": "^0.6.0",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.13.2",
    "@nuxt/test-utils": "^3.14.3",
    "@types/node": "^22.7.6",
    "changelogen": "^0.5.7",
    "eslint": "^9.13.0",
    "nuxt": "^3.13.2",
    "vitest": "^2.1.3"
  },
  "author": {
    "email": "romeo@mendezfuentes.net",
    "name": "Romeo Mendez Fuentes",
    "url": "https://romeo.mendezfuentes.net"
  },
  "homepage": "https://github.com/memotux/nuxt-quasar",
  "repository": {
    "type": "git",
    "url": "https://github.com/memotux/nuxt-quasar.git"
  },
  "keywords": [
    "nuxt",
    "quasar",
    "module",
    "vite",
    "ui"
  ],
  "peerDependencies": {
    "quasar": "^2.16.2",
    "sass": "^1.69.5"
  },
  "packageManager": "pnpm@9.12.2+sha512.22721b3a11f81661ae1ec68ce1a7b879425a1ca5b991c975b074ac220b187ce56c708fe5db69f4c962c989452eee76c82877f4ee80f474cebd61ee13461b6228"
