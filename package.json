{
  "name": "nuxt-quasar-vite",
  "version": "1.1.1",
  "description": "Quasar Framework UI for Nuxt",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "pnpm run prepack && changelogen --release && pnpm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.16.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.2.1",
    "@nuxt/eslint-config": "^1.2.0",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.16.0",
    "@nuxt/test-utils": "^3.17.2",
    "@types/node": "latest",
    "changelogen": "^0.6.1",
    "eslint": "^9.22.0",
    "globby": "^14.1.0",
    "nuxt": "^3.16.0",
    "typescript": "~5.8.2",
    "vitest": "^3.0.8",
    "vue-tsc": "^2.2.8"
  },
  "author": {
    "email": "romeo@mendezfuentes.net",
    "name": "Romeo Mendez Fuentes",
    "url": "https://romeo.mendezfuentes.net"
  },
  "homepage": "https://github.com/memotux/nuxt-quasar",
  "repository": {
    "type": "git",
    "url": "https://github.com/memotux/nuxt-quasar.git"
  },
  "keywords": [
    "nuxt",
    "quasar",
    "module",
    "vite",
    "ui"
  ],
  "peerDependencies": {
    "quasar": "^2.16.2",
    "sass": "^1.69.5"
  },
  "packageManager": "pnpm@9.15.3"
}